<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<module name="avi" version="${project.version}" build-number="${build.number}" image="images/default-16x16.png">
    <description>avi plug-in for vRealize Orchestrator</description>
    <installation mode="${installation.mode}">
        <action resource="packages/${project.artifactId}-package-${project.version}.package" type="install-package"/>
    </installation>
    <finder-datasources>
        <finder-datasource name="main-datasource" adaptor-class="com.avi.avi.aviPluginAdaptor" concurrent-call="false" anonymous-login-mode="internal"/>
    </finder-datasources>
    <scripting-objects>
        <object script-name="_aviSDK" java-class="com.avi.avi.model.SDK" strict="false" is-deprecated="false">
            <description></description>
            <methods>
                <method script-name="createObject" java-name="createObject" return-type="Any" show-in-api="true" is-deprecated="false">
                    <description></description>
                    <parameters>
                        <parameter type="String" name="objectType"/>
                    </parameters>
                </method>
                <method script-name="makeVSObject" java-name="makeVSObject" return-type="com.avi.avi.model.VirtualService" show-in-api="true" is-deprecated="false">
                    <description></description>
                </method>
                <method script-name="makePoolObject" java-name="makePoolObject" return-type="com.avi.avi.model.Pool" show-in-api="true" is-deprecated="false">
                    <description></description>
                </method>
            </methods>
            <singleton script-name="aviSDK" datasource="main-datasource"/>
        </object>
    </scripting-objects>
</module>
